<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_result"
    app:startDestination="@id/ResultFragment">

    <fragment
        android:id="@+id/ResultFragment"
        android:name="com.example.csiro.ResultFragment"
        android:label="@string/result_fragment_label"
        tools:layout="@layout/activity_result">
        <action
            android:id="@+id/action_ResultFragment_to_MainActivity"
            app:destination="@id/MainActivity"/>
    </fragment>

    <activity
        android:id="@+id/MainActivity"
        android:name="com.example.csiro.MainActivity"
        />

</navigation>